{
  "name": "@amokrushin/app-init",
  "version": "0.2.0-debug.3",
  "description": "Graceful shutdown application services",
  "license": "MIT",
  "author": "Anton Mokrushin <anton@mokr.org>",
  "type": "module",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "run-s build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "run-s lint:eslint lint:types",
    "lint:eslint": "eslint lib/",
    "lint:types": "tsc --noEmit",
    "test": "exit 0",
    "postversion": "git push --follow-tags",
    "precommit": "run-s lint test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "dependencies": {
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@amokrushin/eslint-config": "^1.0.0",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@types/node": "^14.14.21",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.1.3"
  },
  "files": [
    "lib/*",
    "types/*"
  ],
  "engines": {
    "node": ">14"
  }
